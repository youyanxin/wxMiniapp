<!--pages/haircut/index.wxml-->
<view class="wrap">
  <view class="swiper">
    <swiper autoplay="true" interval="3000" duration="500">
      <block wx:for="{{bannerList}}" wx:key="*this">
        <swiper-item>
          <view class="swiper-item">
            <image src="{{item}}"></image>
          </view>
        </swiper-item>
      </block>
    </swiper>
  </view>
</view>
